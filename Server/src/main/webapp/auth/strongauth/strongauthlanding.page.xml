<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	  view-id="/auth/strongauth/strongauthlanding.xhtml" action="#{authenticator.authenticateWithOutcome}">
    
    <rewrite pattern="/strongauthlanding" />

    <navigation from-action="#{authenticator.authenticateWithOutcome}">
        <rule if-outcome="failure">
            <redirect view-id="/auth/strongauth/strongauthlogin.xhtml">
                <message severity="ERROR">Failed to authenticate #{messages[faultstring]}</message>
            </redirect>
        </rule>
    </navigation>
</page>
